<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.Controller">
    <children>
        <Button fx:id="versionButton" mnemonicParsing="false" text="Version" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <Button fx:id="createGameButton" mnemonicParsing="false" text="Create game" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <Button fx:id="joinGameButton" mnemonicParsing="false" text="Join game" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <TextField fx:id="textInput" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    </children>
    <columnConstraints>
        <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
    </rowConstraints>
</GridPane>
